<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="javafx.entities.AdminsEntity" table="Admins" schema="Conference_Management">
        <id name="adminId">
            <column name="AdminID" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="Name" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="userName">
            <column name="UserName" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="password">
            <column name="Password" sql-type="text"/>
        </property>
        <property name="email">
            <column name="Email" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>